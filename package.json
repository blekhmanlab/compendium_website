{
  "name": "compendium-website",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "compile": "npx tsx ./compile/index.ts",
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix && prettier **/*.{tsx,ts,css,html,md,yaml} --write --no-error-on-unmatched-pattern",
    "test:lint": "eslint . && prettier **/*.{tsx,ts,css,html,md,yaml} --check --no-error-on-unmatched-pattern",
    "test:types": "tsc",
    "test": "bun run test:lint && bun run test:types",
    "test:spelling": "bunx cspell src/**/*.{tsx,ts}",
    "clean": "rm -rf node_modules dist bun.lockb && bun pm cache rm"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@reactuses/core": "^6.0.1",
    "comlink": "^4.4.2",
    "d3": "^7.9.0",
    "gsap": "^3.12.5",
    "lodash": "^4.17.21",
    "poisson-disk-sampling": "^2.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tippy.js": "^6.3.7",
    "trigram-similarity": "^1.0.7",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@types/d3": "^7.4.3",
    "@types/geojson": "^7946.0.15",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.5",
    "@types/poisson-disk-sampling": "^2.2.4",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "geojson-dissolve": "^3.1.0",
    "globals": "^15.9.0",
    "nodejs-file-downloader": "^4.13.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-jsdoc": "^1.3.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-plugin-svgr": "^4.3.0"
  }
}
